{
    "kafka" : {
        "host_ip": {{ getv "/configuration/exporter/worker/kafka/host/ip" "[\"10.28.11.49:9092\"]" }},
        "topic": "{{ getv "/configuration/exporter/worker/kafka/topic" "topic-test-101" }}",
        "group_id": "{{ getv "/configuration/exporter/worker/kafka/group/id" "task_group_1" }}",
        "offset_reset": "{{ getv "/configuration/exporter/worker/kafka/offest/reset" "earliest" }}",
        "poll_timeout_milliseconds": {{ getv "/configuration/exporter/worker/kafka/poll/timeout/milliseconds" "270000000" }},
        "poll_records": {{ getv "/configuration/exporter/worker/kafka/poll/records" "1" }}
    },
    "input_output": {
        "output_srs": "{{ getv "/configuration/exporter/worker/input/output/output/srs" "EPSG:4326" }}",
        "output_format": "{{ getv "/configuration/exporter/worker/input/output/output/format" "GPKG" }}",
        "external_physical_path": "{{ getv "/configuration/exporter/worker/input/output/external/physical/path" "/ubuntu/outputs"}}",
        "internal_outputs_path": "{{ getv "/configuration/exporter/worker/input/output/internal/outputs/path" "./outputs"}}"
    },
    "logger": {
        "level": "{{ getv "/configuration/exporter/worker/logger/level" "INFO" }}",
        "filename": "{{ getv "/configuration/exporter/worker/logger/filename" "ew_logs.log" }}",
        "backup_count": {{ getv "/configuration/exporter/worker/logger/backup/count" "10" }},
        "max_bytes": {{ getv "/configuration/exporter/worker/logger/max/bytes" "5242880" }}
    },
    "exportstorage": {
        "url": "{{ getv "/configuration/exporter/worker/exportstorage/url" "http://127.0.0.1:8080" }}"
    },
    "gdal": {
        "multithread": {{ getv "/configuration/exporter/worker/gdal/multithread" "true" }},
        "thread_count": {{ getv "/configuration/exporter/worker/gdal/thread/count" "-1" }},
        "aws": {
            "secret_access_key": "{{ getv "/configuration/exporter/worker/gdal/aws/secret/access/key" "minioadmin" }}",
            "access_key_id": "{{ getv "/configuration/exporter/worker/gdal/aws/access/key/id" "minioadmin" }}",
            "s3_endpoint": "{{ getv "/configuration/exporter/worker/gdal/aws/s3/endpoint" "10.15.3.20s:9000" }}",
            "https": "{{ getv "/configuration/exporter/worker/gdal/aws/https" "NO" }}",
            "virtual_hosting": "{{ getv "/configuration/exporter/worker/gdal/aws/virtual/hosting" "FALSE" }}",
            "s3_bucket": "{{ getv "/configuration/exporter/worker/gdal/aws/s3/bucket" "mapping-images" }}"
        }
    },
    "max_attempts": {{ getv "/configuration/exporter/worker/max/attempts" "3" }},
    "mandatory_task_fields": ["fileName", "url", "bbox", "taskId", "directoryName"],
    "storage_provider": "{{ getv "/configuration/exporter/worker/storage/provider" "s3" }}"
}
